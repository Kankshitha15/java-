import java.util.Scanner;
class Division
{
   int num;
   int getNum()
   {
     return num;
   }
   void setNum(int num1)
   {
      if(num1==0)
      {
        num=10;
      }
      else
      {
        num=num1;
      }
  
   }
  public static void main(String args[])
  {
   Scanner scan=new Scanner(System.in);
   int num1 = scan.nextInt();
   int num2 = scan.nextInt();
    
   Division d=new Division();
   d.setNum(num2);
   d.getNum();
  }
}
